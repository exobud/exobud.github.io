{
  "name": "hexo-theme-overdose-starter",
  "version": "1.0.0",
  "hexo": {
    "version": "3.3.8"
  },
  "dependencies": {
    "hexo": "^3.2.0",
    "hexo-deployer-git": "^0.3.0",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-processor-static": "^1.0.0",
    "hexo-renderer-bourbon": "^1.0.4",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-jade": "^0.4.1",
    "hexo-renderer-marked": "^0.2.10",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0"
  },
  "scripts": {
    "deploy": "npm run gh-pages && npm run master",
    "gh-pages": "git add . && git commit -m \"update on `date +\"%Y/%m/%d %H:%M:%S\"`\" && git push origin gh-pages; exit 0",
    "master": "hexo clean && hexo deploy; exit 0",
    "font": "npm run noto && npm run spo-kr && npm run spo-jp",
    "noto": "git clone https://github.com/hyunseob/noto-sans-kr ./themes/overdose/source/libs/noto-sans-kr",
    "spo-kr": "git clone https://github.com/perfectacle/spoqa-han-sans-kr ./themes/overdose/source/libs/spoqa-han-sans-kr",
    "spo-jp": "git clone https://github.com/perfectacle/spoqa-han-sans-jp ./themes/overdose/source/libs/spoqa-han-sans-jp"
  }
}
